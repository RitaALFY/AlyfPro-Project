{% extends 'front/base.html.twig' %}
{% block body %}
    <section id="references" class="text-center">
        <div class="boucle-ref">
        <div class="row">
            <div class="col-md-12 pt-5">
                <div class="row justify-content-center">
                    {% for i in 0..4 %}
                        <div class="col-md-2">
                            <img src="{{ asset('images/ref/' ~ imageNames[i]) }}" alt="Image {{ i }}" class="img-fluid mb-3">
                        </div>
                    {% endfor %}
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12 pt-2">
                <div class="row justify-content-center">
                    {% for i in 5..imageNames|length-1 %}
                        <div class="col-md-2">
                            <img src="{{ asset('images/ref/' ~ imageNames[i]) }}" alt="Image {{ i }}" class="img-fluid mb-3">
                        </div>
                    {% endfor %}
                </div>
            </div>
        </div>
        </div>
    </section>



{% endblock %}